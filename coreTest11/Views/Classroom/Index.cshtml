@model IEnumerable<coreTest11.Models.Classroom>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
                ClassRoom
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @*<a asp-controller="Developments" asp-route-id="@item.DID">@Html.DisplayFor(modelItem => item.DName)</a>*@
                    @item.Name
                    @item.Grade
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.ClassroomID">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.ClassroomID">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.ClassroomID">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
